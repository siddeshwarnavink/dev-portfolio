  "license": "ISC",
  "repository": {
    "url": "https://github.com/siddeshwarnavink/dev-portfolio",
    "type": "git"
  },
  "devDependencies": {
    "concurrently": "^5.2.0",
    "node-sass": "^4.14.1",
    "serve": "^11.3.2"
  },
  "dependencies": {
    "gh-pages": "^3.1.0"
  }
}
