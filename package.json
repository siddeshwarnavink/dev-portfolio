{
  "name": "dev-portfolio",
  "version": "1.0.0",
  "description": "A developer portfolio website template",
  "main": "scss/main.scss",
  "homepage": "https://siddeshwarnavink.github.io/dev-portfolio/",
  "scripts": {
    "start": "concurrently --kill-others \"npm run serve\" \"npm run watch\"",
    "serve": "serve dist/",
    "watch": "node-sass -w scss/ -o dist/css/ --recursive",
    "deploy": "gh-pages -d dist"
  },
